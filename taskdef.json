{
  "family": "ecs-lab",
  "executionRoleArn": "arn:aws:iam::230339925016:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "IMAGE1_NAME",  
      "image": "230339925016.dkr.ecr.us-east-1.amazonaws.com/myrepo:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
